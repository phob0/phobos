{
    "name": "phobos/framework",
    "description": "Phobos Framework",
    "minimum-stability": "dev",
    "require": {
        "laravel/passport": "^7.5",
        "barryvdh/laravel-cors": "^0.11",
        "predis/predis": "^1.1",
        "spatie/laravel-translatable": "^4.2"
    },
    "autoload": {
        "files": [
            "src/app/helpers.php"
        ],
        "psr-4": {
            "Phobos\\Framework\\": "src",
            "Phobos\\Framework\\App\\": "src/app"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Phobos\\Framework\\PhobosServiceProvider"
            ]
        }
    }
}
